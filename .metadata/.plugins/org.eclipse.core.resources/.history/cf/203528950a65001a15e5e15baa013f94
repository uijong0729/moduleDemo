package com.sungnam;
import com.sungnam.util.Calculator;

public class Runner {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Calculator c = new Calculator();
		int result = c.add(4, 5);
		System.out.println(result);
		
		result = c.add(4.5, 5.6);
		System.out.println(result);
	}

}
